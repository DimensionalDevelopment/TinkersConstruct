{
    "schemaVersion": 1,
    "id": "tconstruct",
    "version": "${version}",
    "name": "Tinkers' Construct",
    "description": "A little of this, a little of that, a lot of tinkering, and a lot of tools",
    "authors": [
        "Slime Knights",
        "hydos",
        "waterpicker"
    ],
    "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/tinkers-construct",
        "sources": "https://github.com/DimensionalDevelopment/TinkersConstruct"
    },
    "license": "MIT",
    "icon": "assets/tconstruct/logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "slimeknights.tconstruct.TConstruct",
            "slimeknights.tconstruct.shared.TinkerCommons",
            "slimeknights.tconstruct.shared.TinkerMaterials",
            "slimeknights.tconstruct.fluids.TinkerFluids",
            "slimeknights.tconstruct.gadgets.TinkerGadgets",
            "slimeknights.tconstruct.smeltery.TinkerSmeltery",
            "slimeknights.tconstruct.world.TinkerWorld",
            "slimeknights.tconstruct.world.TinkerStructures",
            "slimeknights.tconstruct.tables.TinkerTables",
            "slimeknights.tconstruct.tools.TinkerToolParts",
            "slimeknights.tconstruct.tools.TinkerTools",
            "slimeknights.tconstruct.tools.TinkerModifiers"
        ],
        "client": [
            "slimeknights.tconstruct.shared.CommonsClientEvents",
            "slimeknights.tconstruct.fluids.FluidClientEvents",
            "slimeknights.tconstruct.gadgets.GadgetClientEvents",
            "slimeknights.tconstruct.smeltery.SmelteryClientEvents",
            "slimeknights.tconstruct.tables.TableClientEvents",
            "slimeknights.tconstruct.shared.TinkerClient",
            "slimeknights.tconstruct.tools.ToolClientEvents",
            "slimeknights.tconstruct.tools.client.ToolRenderEvents",
            "slimeknights.tconstruct.world.WorldClientEvents"
        ]
    },
    "mixins": [
        "tconstruct.mixins.json"
    ],
    "accessWidener": "tconstruct.aw",
    "depends": {
        "fabricloader": ">=0.7.4",
        "fabric": "*",
        "mantle": "*",
        "minecraft": "1.16.x"
    },
    "suggests": {
    },
    "custom": {
        "cardinal-components": [
            "tconstruct:piggyback",
            "dimdoors:counter",
            "dimdoors:chunk_lazily_generated"
        ]
    }
}
